{
  "name": "learning-cypress-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "open": "cypress open",
    "dev": "node_modules\\.bin\\cypress run",
    "test:mocha-reporter": "yarn test --reporter mochawesome --reporter-options reportDir=\"result\",overwrite=false,html=false,json=true",
    "merge-report": "mochawesome-merge \"result/*.json\" > index.json",
    "build-report": "marge index.json",
    "open-report": "yarn merge-report && yarn build-report && serve mochawesome-report",
    "run-test": "cypress run --env baseUrl=https://jsonplaceholder.typicode.com/posts && yarn open-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.7.0",
    "merge": "^2.1.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "serve": "^14.2.1"
  }
}
